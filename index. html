<h4>This program checks if the number is positive, negative or zero.</h4>
<p><i>// input from the user </i><br>
const number = parseInt(prompt(“Enter a number: “));<br>
 <i>// check if number is greater than 0</i><br>  
if (number > 0) {<br>
  console.log(“The number is positive”);
  <br>
}
  <br>
  <i>// check if number is 0.</i>
  <br>
  else if (number == 0){<br>
    console.log(“The number is zero”);
  <br>
  }
  <br>
  <i>// if number is less than 0</i>
  <br>
  else{ <br>
  console.log(“The number is negative”);  <br>
